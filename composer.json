{
    "name": "libre-informatique/varieties-bundle",
    "type": "symfony-bundle",
    "description": "Plant taxonomy management : Family / Genus / Species / Subspecies / Variety",
    "require": {
	"php": ">=5.6",
        "symfony/symfony": "3.2.*",
        "blast-project/core-bundle": "^0.4",
        "blast-project/base-entities-bundle": "^0.4",
        "blast-project/utils-bundle": "^0.4",
        "libre-informatique/media-bundle": "^0.4"
    },
    "require-dev": {
	"symfony/phpunit-bridge": "3.2.*",
        "blast-project/outer-extension-bundle": "^0.4.2"
    },
    "license": "LGPL-3.0",
    "keywords": [
        "e-venement",
        "libio",
        "symfony",
        "doctrine",
        "sonata admin",
        "taxonomy",
        "botanic",
        "family",
        "genus",
        "species",
        "variety"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonyLibrinfoVarietiesBundle",
    "authors": [
        {
            "name": "Marcos Bezerra de Menezes",
            "email": "marcos.bezerra@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Librinfo\\VarietiesBundle\\": "src/",
            "Librinfo\\VarietiesBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    },
    "suggest": {
        "libre-informatique/crm-bundle": "Manage contacts, organisms and more..."
    }
}
